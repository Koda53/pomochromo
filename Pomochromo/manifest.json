{
    "manifest_version": 3,
    "name": "Pomochromo",
    "description": "add a extention pop up Timer",
    "version": "1.0",
    
    "action":{
      "default_popup": "index.html",
      "default_title": "Pomochromo",
      "default_icon": "images/popcat.png"
    },
   
  "content_scripts": [
        {
          "matches": ["<all_urls>" ],
          "js": ["blocker.js"]
        }
  ],

  "permissions": [
    "storage"
  ],

  "background": {
    "serivce-worker": "script.js"
  }

}